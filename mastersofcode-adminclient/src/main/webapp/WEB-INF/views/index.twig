{% extends "bootstrap.twig" %}

{% block title %}
    {{ test }}
{% endblock %}

{% block more_css %}
    <link href="/assets/common/css/default.css" rel="stylesheet"/>
    <link href="/assets/common/css/bootstrap/sb-admin/sb-admin.css" rel="stylesheet"/>
    <link href="/assets/common/css/bootstrap/sb-admin/font-awesome/css/font-awesome.min.css" rel="stylesheet"/>
    <link href="/assets/producer/css/dashboard.css" rel="stylesheet"/>
    
    <link rel="stylesheet" href="/assets/common/fancybox/jquery.fancybox.css" type="text/css" media="screen" />  
{% endblock %}

{% block more_js %}
    <script type="text/javascript" src="/assets/producer/js/producer.js"></script>
    <script type="text/javascript" src="/assets/common/fancybox/jquery.fancybox.pack.js"></script>
    <script type="text/javascript">
	$(document).ready(function() {
		$(".fancybox").fancybox();
	});
    </script>
{% endblock %}

{% block body %}
    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" href="/app/producer/dashboard">{{ test }}</a>
            </div>
            
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>    
                        <a href="/logout"><i class="fa fa-sign-out"></i> {{ Logout }}</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>

        <div id="page-wrapper">

            <div class="container-fluid">
                
                {% block heading %}
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            {{ test }}
                        </h1>
                    </div>
                </div>
                {% endblock %}
                
                {% block content %}
                <div class="row">
                    <div class="col-lg-12">
                        <p>
                            {{ test }}
                        </p>
                    </div>
                </div>
                {% endblock %}
                
            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
{% endblock %}